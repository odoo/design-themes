<?xml version="1.0" encoding="utf-8"?>
<odoo>

<asset id="theme_kea.primary_variables_scss" name="Primary variables SCSS">
    <field name="key">theme_kea.primary_variables_scss</field>
    <bundle>web._assets_primary_variables</bundle>
    <path>theme_kea/static/src/scss/primary_variables.scss</path>
</asset>

<asset id="theme_kea.secondary_variables_scss" name="Secondary variables SCSS">
    <field name="key">theme_kea.secondary_variables_scss</field>
    <bundle>web._assets_secondary_variables</bundle>
    <path>theme_kea/static/src/scss/secondary_variables.scss</path>
</asset>

<asset id="theme_kea.bootstrap_overridden_scss" name="Bootstrap overridden SCSS">
    <field name="key">theme_kea.bootstrap_overridden_scss</field>
    <bundle directive="prepend">web._assets_frontend_helpers</bundle>
    <path>theme_kea/static/src/scss/bootstrap_overridden.scss</path>
</asset>

</odoo>
