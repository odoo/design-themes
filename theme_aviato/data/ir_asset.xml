<?xml version="1.0" encoding="utf-8"?>
<odoo>

<asset id="theme_aviato.primary_variables_scss" name="Primary variables SCSS">
    <field name="key">theme_aviato.primary_variables_scss</field>
    <bundle>web._assets_primary_variables</bundle>
    <path>theme_aviato/static/src/scss/primary_variables.scss</path>
</asset>

<asset id="theme_aviato.bootstrap_overridden_scss" name="Bootstrap overridden SCSS">
    <field name="key">theme_aviato.bootstrap_overridden_scss</field>
    <bundle directive="prepend">web._assets_frontend_helpers</bundle>
    <path>theme_aviato/static/src/scss/bootstrap_overridden.scss</path>
</asset>

</odoo>
