<?xml version="1.0" encoding="utf-8"?>
<odoo>

<asset id="theme_bookstore.primary_variables_scss" name="Primary variables SCSS">
    <field name="key">theme_bookstore.primary_variables_scss</field>
    <bundle>web._assets_primary_variables</bundle>
    <path>theme_bookstore/static/src/scss/primary_variables.scss</path>
</asset>

<asset id="theme_bookstore.bootstrap_overridden_scss" name="Bootstrap overridden SCSS">
    <field name="key">theme_bookstore.bootstrap_overridden_scss</field>
    <bundle directive="prepend">web._assets_frontend_helpers</bundle>
    <path>theme_bookstore/static/src/scss/bootstrap_overridden.scss</path>
</asset>

<asset id="theme_bookstore.compatibility_saas_11_4_variables_scss" name="Compatibility saas 11 4 variables SCSS" active="False">
    <field name="key">theme_bookstore.compatibility_saas_11_4_variables_scss</field>
    <bundle>web._assets_primary_variables</bundle>
    <path>/theme_bookstore/static/src/scss/compatibility-saas-11.4-variables.scss</path>
</asset>

</odoo>
